local function getScoreboard(player:player, economy:string) :: string:
    if {_economy} = ("money" or "credits"):
        return "%{-economy::%{_economy}%::symbol}%%regex({data::%{_economy}%::%{_player}'s uuid%}*1)%&f %small({_economy})%"
    else:
        return "&f%small({_economy})% %{-economy::%{_economy}%::symbol}%%regex({data::%{_economy}%::%{_player}'s uuid%}*1)%"