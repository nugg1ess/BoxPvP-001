command /item <text>:
    permission: op
    usage: &7/&fitem &7[<&f1&7/&f2&7/&f3&7>]
    trigger:
        if arg-1 is "1":
            item1(player)
        else if arg-1 is "2":
            item2(player)
#        else if arg-1 is "3":
#            item3(player)
#        else if arg-1 is "4":
#            item4(player)

function item1(p: player):
    create a gui with chest inventory with 5 rows named "&8&l             TOOLS":

        # first row

        make gui 2 with stonePickaxeTier1():
            give stonePickaxeTier1() to {_p}
